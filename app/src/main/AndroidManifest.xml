<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.mmatuszek.moneysaver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.AddCostTypeActivity" />

        <meta-data
            android:name="roboguice.modules"
            android:value="pl.mmatuszek.moneysaver.infrastructure.IOCModule" />

        <activity android:name=".view.AddCostActivity" />
        <activity android:name=".view.ShowCostsActivity"></activity>
    </application>

</manifest>